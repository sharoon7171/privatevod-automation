	function addToVODCart(ID, options, type, isScene, sceneDefaultURL)
	 {
		var cartItemType;
		var sceneFrame;

		$("#cartitemdescription").html("");
		$("#carttoastscreenshot").hide();
		$("#carttype").html("");
		$("#toastitemtype").html("");
		$("#stockstatus").html("");
		$("#toastprediscountprice").html("");
		$("#toastprice").html("");
		$("#carttotal").html("");
		$("#totalcartitems").html("");
		$("#checkout").attr("href", "/");

		 $.ajax({
			url: "/Item/AddToCart",    
			dataType: "json",
			data: ({
				itemID: ID, 
				options: options,
				type: type
			}),
			success: function(data) {
				//alert(data);
				if(data){
					if(data.Error == null || data.Error == "")
					{
						$("#carttype").html(`
							<p class="pr-1 pr-lg-0">
								Item added to your
							</p>
							<h3>
								Digital Cart!
							</h3>
						`);
						$("#cartitemdescription").html(data.Title);
						$("#toastitemtype").html(data.Item_Type);
						$("#toastcarttypemessage").show();
						$("#cartButtons").html(`
							<a class="btn btn-secondary m-1"
								data-Category="Item Page" data-Label="Toast - Continue Shopping"
								href="javascript://" onclick="closeCartToast();" > 
								Continue Shopping
							</a>
							<a class="btn btn-success m-1"
								id="checkout"
								data-Category="Item Page" data-Label="Toast - Checkout">
								Checkout
							</a>						
						`);
                        //alert(data);
				    if(data.preDiscountPrice != data.price.toFixed(2)){
						if(data.Coupon_Applied){
							$("#toastdiscounttype").append(`
								<span class="text-sale">
								Coupon Applied!
								</span>
							`);
						}
					if(data.Sale_Applied){
							$("#toastdiscounttype").append(`
								<span class="text-sale">
								On Sale!
								</span>
							`);
						}
						if(data.Elite_Applied){
							$("#toastdiscounttype").append(`
								<span class="text-sale">
								Member Discount!
								</span>
							`);
						}

						$("#toastprediscountprice").html(`
							<br />
							Regular Price:
							<s>
							$${data.preDiscountPrice.toFixed(2)}
							</s>
						`);
						}
						if(isScene)
						{
							$("#carttoastboxcover").hide();
							if(sceneDefaultURL != null)
							{
								$("#screenshottoast").attr("src", sceneDefaultURL);
								$("#carttoastscreenshot").show();
							}
						}
						else{
							$("#carttoastscreenshot").hide();
							$("#carttoastboxcover").show();
						}
						$("#toastprice").html(`
							<span class="item-price">
								$${data.price.toFixed(2)}
							</span>
						`);
						$("#carttotal").html(`
							Cart Total:
							<span class="cart-total-price">
								$${data.Grand_Total.toFixed(2)}
							</span>
						`);
						$("#totalcartitems").html(`
							Total Items In Digital Cart
							<span class="badge badge-success">
								${data.CartCount}
							</span>
						`);
						$("#checkout").attr("href", "/vodcart")
						$("#carttoast").show();
					}
					else{
						$("#toasterror").html(data.Error);
						$("#carttoastboxcover").hide();
						$("#toastcarttypemessage").hide();
						$("#checkout").attr("href", "/vodcart")
						$("#toasterror").show();
						$("#carttoast").show();
					}
				}
			}
		});
	 }

	 function AddPhysicalProductToast(ID)
	 {
	 	$("#cartitemdescription").html("");
		$("#stockstatus").html("");
		$("#toastdiscounttype").html("");
		$("#carttype").html("");
		$("#toastitemtype").html("");
		$("#toastprediscountprice").html("");
		$("#toastprice").html("");
		$("#carttotal").html("");
		$("#totalcartitems").html("");
		$("#carttoastscreenshot").hide();
		$("#checkout").attr("href", "/");

		  $.ajax({
				url: "/Cart/AddItemCartToast",    
				dataType: "json",
				data: ({
					itemID: ID
				}),
				success: function(data) {
					if(data){
					$("#carttoastboxcover").show();
					$("#carttype").html(`
						<p class="pr-1 pr-lg-0">
							Item added to your
						</p>
						<h3>
							Physical Cart!
						</h3>
					`);
					$("#toastcarttypemessage").show();
					$("#cartitemdescription").html(data.Title);
					$("#cartButtons").html(`
						<a class="btn btn-secondary m-1"
							data-Category="Item Page" data-Label="Toast - Continue Shopping"
							href="javascript://" onclick="closeCartToast();" > 
							Continue Shopping
						</a>
						<a class="btn btn-success m-1"
							id="checkout"
							data-Category="Item Page" data-Label="Toast - Checkout">
							Checkout
						</a>						
					`);
					
					
					
					
					$("#toastitemtype").html(data.Item_Type);
					if(data.Item_Type == "DVD-Video")
					{
						
					}
					else if(data.Item_Type == "Blu-ray"){
						
					}
					$("#toastprice").html(`
						<span class="item-price">
							$${data.price.toFixed(2)}
						</span>
					`);
					$("#carttotal").html(`
						Cart Total:
						<span class="cart-total-price">
							$${data.Grand_Total.toFixed(2)}
						</span>
					`);
					$("#totalcartitems").html(`
						Total Items In Physical Cart
						<span class="badge badge-success">
							${data.CartCount}
						</span>
					`);
					$("#checkout").attr("href", "/cart/cartview")
					$("#carttoast").show();
				}
			}
		});
	 }

	function closeCartToast()
	 {
	 	$("#cartitemdescription").html("");
		$("#carttoastscreenshot").hide();
		$("#carttype").html("");
		$("#stockstatus").html("");
		$("#toastdiscounttype").html("");
		$("#toastitemtype").html("");
		$("#toastprediscountprice").html("");
		$("#toastprice").html("");
		$("#carttotal").html("");
		$("#totalcartitems").html("");
		$("#checkout").attr("href", "/")
		$("#carttoast").hide();
	 }



	</script>


		       
