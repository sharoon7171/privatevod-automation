<script>
	// Non-VUE => Vue helpers
    // Component library not loaded until window.vueApp is exposed
    const waitForStore = new Promise((resolve, reject) => {
		(function wait() {
		  if(window.vueApp?.config?.globalProperties?.$store) return resolve(window.vueApp.config.globalProperties.$store)
		  setTimeout(wait, 5);
		})()
	})
	
	  // Init dat
	waitForStore.then(vueStore => { 
		vueStore.cart.hardgoods = 0
		vueStore.cart.vods = 0
	})
	
	function cancelEvent(e) {
		var ev = (e||window.event);
		if(ev) {
			ev.preventDefault();
			ev.stopPropagation();
		}
		return ev
	}

	function addToDigitalCart(e, id, contentType, termType, term, hideToast) {
		var ev = cancelEvent(e);
		return waitForStore.then(vueStore => { 
			return vueStore.cart.addToCart({id, Ref_Type: contentType, termType, term, hideToast})
		}).then(() => {
			return false;
		})
		
	}

	function callComponentMethod(ref, method, params) {
		waitForStore.then(function(store) {
		  if(window.vueApp._instance.refs[ref] && window.vueApp._instance.refs[ref][method]) {
			window.vueApp._instance.refs[ref][method](params)
		  }
		})
	  }
</script>
		
